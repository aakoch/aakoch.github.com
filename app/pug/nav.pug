mixin nav
    nav.navbar
        .container
            .navbar-header
                button.navbar-toggle.collapsed(type=button data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
                    span.sr-only 1
                    span.icon-bar 2
                    span.icon-bar 3
                a.navbar-brand Project Name
            .collapse.navbar-collapse#navbar
                ul.nav.navbar-nav
                    li.active 
                        a Home
                    li 
                        a About
                    li 
                        a Contact
                    