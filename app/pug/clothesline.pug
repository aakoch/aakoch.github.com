extends ./clothesline_base.pug

block content
  span#golink
    a(href="javascript:$('hr + div').addClass('moving')") Go!
  span
    a(href="javascript:$('.moving').removeClass('moving')") Stop!
  .container
    hr(noshade)
    .item.moving
      img(src='https://www.dogtired.org/alero/summer_01/frontquarter.jpg')
      .picture
        p My Alero
        
